##
# [ ~/.config/zsh/android.zsh ]
# Set path to include bins used for android development
# Sourced by .zshrc
##

export ANT_HOME="/usr/share/ant"
export ANDROID_HOME="/home/tbck/android/sdk"
export ANDROID_SDK="/home/tbck/android/sdk"
export PATH="$PATH:$ANT_HOME/bin:$ANDROID_HOME/tools:$ANDROID_HOME/platform-tools"
